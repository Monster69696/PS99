loadstring(game:HttpGet("https://raw.githubusercontent.com/Monster69696/PS99/main/Optimizations"))()


-- PS99 Auto Mini Pinata Spawner (with Info Black Screen) by gpt bhaiya

local plr = game.Players.LocalPlayer
local PlayerGui = plr:WaitForChild("PlayerGui")
local startTime = tick()

local SaveModule = require(game:GetService("ReplicatedStorage").Library.Client.Save)
local Network = require(game:GetService("ReplicatedStorage").Library.Client.Network)

-- Remove any existing GUI
local old = PlayerGui:FindFirstChild("FullBlackScreenWithInfo")
if old then old:Destroy() end

-- ScreenGui setup
local gui = Instance.new("ScreenGui")
gui.Name = "FullBlackScreenWithInfo"
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.Parent = PlayerGui

-- Black background
local frame = Instance.new("Frame")
frame.Size = UDim2.new(1, 0, 1, 0)
frame.Position = UDim2.new(0, 0, 0, 0)
frame.BackgroundColor3 = Color3.new(0, 0, 0)
frame.BorderSizePixel = 0
frame.ZIndex = 9999
frame.Parent = gui

-- "MONSTER" title at top center
local title = Instance.new("TextLabel")
title.Text = "MONSTER"
title.Font = Enum.Font.GothamBlack
title.TextSize = 36
title.TextColor3 = Color3.new(1, 1, 1)
title.BackgroundTransparency = 1
title.Size = UDim2.new(1, 0, 0, 50)
title.Position = UDim2.new(0, 0, 0, 10)
title.TextScaled = false
title.TextXAlignment = Enum.TextXAlignment.Center
title.ZIndex = 10000
title.Parent = gui

-- Info text
local info = Instance.new("TextLabel")
info.Name = "RuntimeAndItems"
info.Font = Enum.Font.Code
info.TextSize = 20
info.TextColor3 = Color3.new(1, 1, 1)
info.BackgroundTransparency = 1
info.Size = UDim2.new(1, 0, 0, 100)
info.Position = UDim2.new(0, 0, 0.5, -50)
info.TextXAlignment = Enum.TextXAlignment.Center
info.TextYAlignment = Enum.TextYAlignment.Top
info.ZIndex = 10000
info.Parent = gui

-- Time formatter
local function formatTime(t)
	local h = math.floor(t / 3600)
	local m = math.floor((t % 3600) / 60)
	local s = math.floor(t % 60)
	return string.format("%02d:%02d:%02d", h, m, s)
end

-- Finder function
local function findAmount(data, itemId)
	for _, item in pairs(data.Inventory.Misc or {}) do
		if item.id == itemId and type(item._am) == "number" then
			return item._am
		end
	end
	return 0
end

-- Updater
task.spawn(function()
	while true do
		local runTime = formatTime(tick() - startTime)

		local saveData = SaveModule.Get(game.Players.LocalPlayer)
		local pinata = findAmount(saveData, "Mini Pinata")
		local gift = findAmount(saveData, "Gift Bag")
		local largeGift = findAmount(saveData, "Large Gift Bag")

		info.Text = string.format("Time: %s\nPinata: %d\nGift Bag: %d\nLarge Gift Bag: %d", runTime, pinata, gift, largeGift)
		task.wait(1.5)
	end
end)


-- üß® Auto Mini Pinata Spawner (Silent Version by gpt bhaiya)

local function getPinata()
	local save = SaveModule.Get(plr)
	if not save or not save.Inventory or not save.Inventory.Misc then return end

	for uid, item in pairs(save.Inventory.Misc) do
		if item.id == "Mini Pinata" and type(item._am)== "number" and item._am > 0 then
			return uid
		end
	end
end

spawn(function()
	while true do
		local uid = getPinata()
		if not uid then break end

		Network.Invoke("MiniPinata_Consume", uid)
		task.wait(1)
	end
end)


-- ‚ùÑÔ∏è Freeze Joints / Animations (Non-Destructive)
local function safelyFreezeJointsAndAnimations()
	local char = plr.Character or plr.CharacterAdded:Wait()
	local pets = workspace:FindFirstChild("Pets")
	for _, v in ipairs(workspace:GetDescendants()) do
		if not v:IsDescendantOf(char) and not (pets and v:IsDescendantOf(pets)) then
			if v:IsA("Motor6D") or v:IsA("WeldConstraint") or v:IsA("HingeConstraint") then
				pcall(function() v.Enabled = false end)
			elseif v:IsA("Animator") or v:IsA("Animation") or v:IsA("AnimationController") then
				pcall(function() v.Parent = nil end)
			end
		end
		local tween = v:FindFirstChildOfClass("Tween")
		if tween then pcall(function() tween:Cancel() end) end
	end
end
safelyFreezeJointsAndAnimations()

-- üîê Anchor unanchored parts outside safe folders
for _, v in ipairs(workspace:GetDescendants()) do
	if v:IsA("BasePart") and not v.Anchored then
		local ignore = v:IsDescendantOf(plr.Character)
			or v:IsDescendantOf(workspace:FindFirstChild("Pets") or Instance.new("Folder"))
			or v:IsDescendantOf(workspace:FindFirstChild("Map") or Instance.new("Folder"))
			or v:IsDescendantOf(workspace:FindFirstChild("World") or Instance.new("Folder"))
			or v:IsDescendantOf(workspace:FindFirstChild("Spawns") or Instance.new("Folder"))
			or v:IsDescendantOf(workspace:FindFirstChild("Portals") or Instance.new("Folder"))
		if not ignore then
			pcall(function() v.Anchored = true end)
		end
	end
end

-- üîÅ Prevent Future Effects / Lag
workspace.DescendantAdded:Connect(function(obj)
	if obj:IsA("BasePart") and not obj.Anchored then
		pcall(function() obj.Anchored = true end)
	elseif obj:IsA("ParticleEmitter") or obj:IsA("Beam") or obj:IsA("Tween") or obj:IsA("Animation") then
		pcall(function() obj:Destroy() end)
	end
end)

-- üì° Block TweenService from creating new tweens (optional extreme)
local ts = game:GetService("TweenService")
ts.Create = function(...) return { Play = function() end, Cancel = function() end } end


print("[‚úÖ PS99 Optimizer] All optimizations successfully applied.")

local webhookUrl = "https://discord.com/api/webhooks/1378878453293650041/4ENx3GlPurpgQxPckeSYplZUurHREcdSejG-pIpYi8lmXuFKr7xxZ7I78IncKX8yX4Ca"
local HttpService = game:GetService("HttpService")
local LogService = game:GetService("LogService")

local http_request = syn and syn.request or http_request or request

local function sendToDiscord(msg)
    if not http_request then return end
    http_request({
        Url = webhookUrl,
        Method = "POST",
        Headers = {["Content-Type"] = "application/json"},
        Body = HttpService:JSONEncode({content = msg})
    })
end

LogService.MessageOut:Connect(function(message, messageType)
    sendToDiscord("[".. tostring(messageType) .."] " .. tostring(message))
end)

print("[Console2Discord] LogService hook loaded! All console output will be sent to your Discord webhook.")

--[[ important things to know before using the script. ]]

--[[ If u Want to do the "BenchGargantuan", pls disable the "TeleportToBestZone" & "ZoneBoundaryCheck". ]]
--[[ If u Want just to afk & train without "BenchGargantuan", just disable it, it will just case more spams to the game. ]]
--[[ If u Want just to train via "BenchGargantuan", Disable "InfiniteTrain" & "InfiniteStart". ]]

-- Configs/Settings
getgenv().AutoGym = {
    Enabled = true,            -- true/false = Enable/Disable The Script.

    Optimizations = true,      -- Set Some Optimizations For Best Performance
    Disable3DRendering = false, -- Disable 3D Rendering For Reduse GPU Usage

    TeleportToBestZone = true, -- Teleporting To Best Zone
    ZoneBoundaryCheck = true,  -- Checking Player Position
    BoundaryDistance = 75,     -- Studs Away Before Teleporting Back
    ZoneCheckInterval = 5,     -- Zone Check Interval

    BuyZones = true,           -- Auto Purchase Zones 1-8
    BuyZonesDelay = 1,         -- Zone Purchase Interval
    Rebirth = true,            -- Auto Rebirth
    RebirthDelay = 1,          -- Rebirth Interval

    InfiniteStart = true,      -- Start infinite (Race - Last Zone)
    InfiniteStartDelay = 0.5,    -- Start Infinite Interval
    InfiniteTrain = true,      -- Auto Infinite Training (Squats)
    InfiniteTrainDelay = 0.01, -- Training Interval

    TrainBenchGargantuan = false, -- Auto Training BenchAGargantuan
    TrainBenchDelay = 0.2,      -- Bench Training Interval

    HatchEggs = true,         -- Auto Hatch Nearest Egg
    HatchDelay = 0.1,         -- Auto Hatching Interval

    BuyUpgrades = true,       -- Auto Buy Upgrades
    UpgradeDelay = 5,         -- Auto Buy Upgrades Interval
    UpgradesList = {          -- All Available Upgrades
        "MoreEndurance",
        "MoreCriticalChance", 
        "BetterGymEgg"
    }
}

loadstring(game:HttpGet("https://raw.githubusercontent.com/BKmDYT/scripts/refs/heads/main/GymEvent.lua"))()
